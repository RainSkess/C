#include <stdio.h>

int main() {
    int N = 0;
    int i = 0;
    int k = 0;
    int j = 0;
    int n = 0;
    scanf("%d", &N);
    char arr[13] = { 0 };
    for (i = 0; N > 0; n++) {
        if (k != 0 && k % 3 == 0)
            arr[i++] = ',';
        arr[i++] = N % 10 + '0';
        N = N / 10;
        k++;
    }
    j = i - 1;
    for (; j >= 0; j--)
    {
        printf("%c", arr[j]);
    }
    return 0;
}
